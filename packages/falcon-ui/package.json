{
  "name": "@deity/falcon-ui",
  "license": "OSL-3.0",
  "version": "0.1.1",
  "homepage": "https://falcon-ui.docs.deity.io/",
  "repository": "https://github.com/deity-io/falcon/tree/master/packages/falcon-ui",
  "main": "dist/falcon-ui.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:tsDeclarations && npm run size",
    "build:tsDeclarations": "tsc",
    "build:esm": "babel src -d dist -x .ts,.tsx -s",
    "build:cjs": "rollup -c",
    "size": "size-limit",
    "start": "docz dev",
    "watch": "babel src -d dist -x .ts,.tsx -s --watch",
    "docs": "rimraf .docz && docz build"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@deity/falcon-theme-editor": "^0.1.1",
    "@mdx-js/tag": "0.16.1",
    "@types/react": "16.7.17",
    "@types/react-dom": "16.0.11",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-module-resolver": "3.1.1",
    "docz": "0.13.7",
    "docz-core": "0.13.7",
    "docz-theme-default": "0.13.7",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-feather": "^1.1.3",
    "rimraf": "2.6.2",
    "rollup": "0.66.4",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-node-resolve": "3.4.0",
    "size-limit": "0.20.1",
    "typescript": "3.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@emotion/cache": "^10.0.0",
    "@emotion/core": "^10.0.5",
    "@emotion/is-prop-valid": "^0.7.3",
    "@emotion/styled-base": "^10.0.5",
    "csstype": "^2.5.7",
    "deepmerge": "^2.2.1",
    "emotion-theming": "^10.0.5",
    "is-plain-object": "^2.0.4",
    "react-powerplug": "1.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "gzip": false,
      "limit": "60 KB"
    }
  ]
}
